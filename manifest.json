{
    "name": "Chrome Panels",
    "version": "0.1",
    "description": "Create panels from links",
    "background": {
        "scripts": ["js/vars.js", "js/background.js"]
    },
    "permissions": [
        "contextMenus",
        "tabs"
    ],
    "browser_action": {
        "default_icon": "img/128x128.png"
    },
    "icons": {
        "128": "img/128x128.png"
    },
    "manifest_version": 2
}

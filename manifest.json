{
  "name": "Chrome Panels",
  "version": "0.13",
  "manifest_version": 2,
  "description": "Create panels from links",
  "background": {
    "scripts": ["/js/background.js"]
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "img/128x128.png"
  },
  "icons": {
    "128": "img/128x128.png"
  },
  "options_ui": {
    "page": "./options.html",
    "chrome_style": true
  }
}

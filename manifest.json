{
    "name": "Pie Panel",
    "version": "0.1",
    "description": "Create panels from links",
    "background": {
        "scripts": ["js/background.js"]
    },
    "permissions": [
        "contextMenus",
        "tabs"
    ],
    "browser_action": {
        "default_icon": "img/add.png"
    },
    "icons": {
        "16": "img/add.png",
        "48": "img/add.png",
        "128": "img/add.png"
    },
    "manifest_version": 2
}
